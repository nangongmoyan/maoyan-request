{
  "version": "0.5.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "changeset": "changeset",
    "version": "changeset version && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "maoyan-request",
  "author": "nangongmoyan",
  "module": "dist/maoyan-request.esm.js",
  "size-limit": [
    {
      "path": "dist/maoyan-request.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/maoyan-request.esm.js",
      "limit": "10 KB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nangongmoyan/maoyan-request.git"
  },
  "bugs": {
    "url": "https://github.com/nangongmoyan/maoyan-request/issues"
  },
  "homepage": "https://github.com/nangongmoyan/maoyan-request#readme",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^9.0.0",
    "@types/uuid": "^9.0.7",
    "husky": "^8.0.3",
    "size-limit": "^9.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "axios": "^1.2.0-alpha.1",
    "moyan-utils": "^0.0.20",
    "uuid": "^9.0.1"
  }
}